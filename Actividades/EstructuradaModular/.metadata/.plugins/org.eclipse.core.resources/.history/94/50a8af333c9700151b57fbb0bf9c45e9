teclado.*/
	public static void main(String[] args) {

		/**DECLARACION VARIABLES Y OBJETOS*/
		int num;
		
		/**PEDIMOS DATOS*/
		num=Teclado.numeroInt("Introduce un número entero positivo", 0,Limite.MAYOR_IGUAL);

	}
	
	

}
