/**
 * Clase que contiene los códigos para colorear el tablero.
 * @author joaquin
 *
 */
public class Constantes{
	public static final String ANSI_RESET="\u001B[0m";
	public static final String ANSI_PCNEGRA="\u001B[0;34;47m";
	public static final String ANSI_PCBLANCA="\u001B[0;34;m";
	public static final String ANSI_USUARIO1NEGRA="\u001B[1;31;47m";
	public static final String ANSI_USUARIO1BLANCA="\u001B[1;31;m";
}